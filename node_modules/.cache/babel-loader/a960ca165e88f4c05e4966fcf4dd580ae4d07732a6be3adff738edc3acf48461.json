{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HeaderComponent from './HeaderComponent.vue';\nimport axios from 'axios';\nexport default {\n  name: 'UpdateComponent',\n  components: {\n    HeaderComponent\n  },\n  data() {\n    return {\n      restaurant: {\n        name: '',\n        address: '',\n        contact: ''\n      }\n    };\n  },\n  methods: {\n    async updateRestaurant() {\n      console.warn(this.restaurant);\n      const result = await axios.post(\"http://localhost:3000/restaurant/\" + this.$route.params.id, {\n        name: this.restaurant.name,\n        address: this.restaurant.address,\n        contact: this.restaurant.contact\n      });\n      if (result.status == 200) {\n        this.$router.push('Home');\n      }\n    }\n  },\n  //life cycle method run when page is load\n  async mounted() {\n    let user = localStorage.getItem('user-info');\n    if (!user) {\n      this.$router.push('SignUp');\n    }\n    const result = await axios.get(\"http://localhost:3000/restaurant/\" + this.$route.params.id);\n    /////////Data Prefilling////////////\n    this.restaurant = result.data;\n  }\n};","map":{"version":3,"names":["HeaderComponent","axios","name","components","data","restaurant","address","contact","methods","updateRestaurant","console","warn","result","post","$route","params","id","status","$router","push","mounted","user","localStorage","getItem","get"],"sources":["/home/teresol/Documents/FrontEnd/ResturantApp/src/components/UpdateComponent.vue"],"sourcesContent":["<template>\n   \n   <div>\n<HeaderComponent />\n    <h1> Welcome to Update  Restaurant Page </h1>\n    <form class=\"update\">\n        <!--------------v-model is two way binding data is insert in field and also get data from field------------------->\n        <input type=\"text\" placeholder=\"Enter Name\" v-model=\"restaurant.name\" name=\"name\"/>\n        <input type=\"text\" placeholder=\"Enter Address\"  v-model=\"restaurant.address\" name=\"address\"/>\n        <input type=\"text\" placeholder=\"Enter Contact\"  v-model=\"restaurant.contact\" name=\"contact\"/>\n        <button type=\"button\" v-on:click=\"updateRestaurant\">Update  Restaurant</button>\n    </form>\n   </div>\n\n    \n</template>\n<script>\nimport HeaderComponent from './HeaderComponent.vue'\nimport axios from 'axios';\nexport default{\n    name:'UpdateComponent',\n    components:{\nHeaderComponent\n    },    data(){\n        return{\n            restaurant:{\n                name:'',\n                address:'',\n                contact:''\n            }\n        }\n    },\n    methods:{\n      async updateRestaurant(){\n        console.warn(this.restaurant)\n        const result =await axios.post(\"http://localhost:3000/restaurant/\"+this.$route.params.id,{\n                name:this.restaurant.name,\n                address:this.restaurant.address,\n                contact:this.restaurant.contact\n            });\n            if(result.status==200){\n                this.$router.push('Home');\n            }\n        }\n    },\n     //life cycle method run when page is load\n   async  mounted(){\n        let user= localStorage.getItem('user-info');\n        if(!user){\n            this.$router.push('SignUp');\n\n        }\n        const result = await axios.get(\"http://localhost:3000/restaurant/\"+this.$route.params.id);\n/////////Data Prefilling////////////\n        this.restaurant=result.data\n    }\n}\n</script>"],"mappings":";AAiBA,OAAOA,eAAc,MAAO,uBAAsB;AAClD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAc;EACVC,IAAI,EAAC,iBAAiB;EACtBC,UAAU,EAAC;IACfH;EACI,CAAC;EAAKI,IAAIA,CAAA,EAAE;IACR,OAAM;MACFC,UAAU,EAAC;QACPH,IAAI,EAAC,EAAE;QACPI,OAAO,EAAC,EAAE;QACVC,OAAO,EAAC;MACZ;IACJ;EACJ,CAAC;EACDC,OAAO,EAAC;IACN,MAAMC,gBAAgBA,CAAA,EAAE;MACtBC,OAAO,CAACC,IAAI,CAAC,IAAI,CAACN,UAAU;MAC5B,MAAMO,MAAK,GAAG,MAAMX,KAAK,CAACY,IAAI,CAAC,mCAAmC,GAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,EAAC;QACjFd,IAAI,EAAC,IAAI,CAACG,UAAU,CAACH,IAAI;QACzBI,OAAO,EAAC,IAAI,CAACD,UAAU,CAAC